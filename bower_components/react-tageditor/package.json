{
  "name": "react-tageditor",
  "version": "0.1.1",
  "description": "A tag editor built with react.js",
  "main": "dist/TagEditor.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-js & npm run build-css",
    "build-js": "webpack",
    "build-css": "sass --sourcemap=none --scss --update src/style/:dist/style/",
    "build-example": "sass --sourcemap=none --load-path=bower_components/neutron/  --style compressed -scss --update examples/style/:examples/style/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NdYAG/react-tageditor.git"
  },
  "keywords": [
    "react",
    "tag",
    "tageditor",
    "react-component"
  ],
  "author": "NdYAG",
  "bugs": {
    "url": "https://github.com/NdYAG/react-tageditor/issues"
  },
  "homepage": "https://daix.me/react-tageditor",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "expose-loader": "^0.7.0",
    "node-libs-browser": "^0.5.2",
    "webpack": "^1.9.10"
  },
  "dependencies": {
    "react": "^0.13.3"
  }
}
